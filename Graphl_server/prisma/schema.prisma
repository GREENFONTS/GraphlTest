generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model author {
  age   Int?
  id    String  @id @default(cuid())
  name  String?
  books books[]
}

model books {
  authorid String?
  genre    String?
  id       String @id @default(cuid())
  name     String?  
  author   author? @relation(fields: [authorid], references: [id])
}